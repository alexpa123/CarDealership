@model List<CarDealershipNew.Models.Models.Special>

@{
    ViewBag.Title = "Specials";
}

<h2>Specials</h2>
<div class="container">
    <div class="row">
        <div class="col-sm-12">
            @foreach (var special in Model)
            {
                <div class="row specials">
                    <div class="col-sm-2">
                        <p>Dollar Sign</p>
                    </div>
                    <div class="col-sm-10">
                        <h1>@special.Title</h1>
                        <p>@special.Description</p>
                    </div>
                </div>
            }

        </div>
    </div>
</div>
